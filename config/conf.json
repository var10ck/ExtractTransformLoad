{
  "pathToWriteParquet": "data_files\\parquet",
  "tables": [
    {
      "name": "Rate",
      "path": "data_files\\csv\\Courses.csv",
      "schema": "data_files\\json\\CoursesSchema.json",
      "sep": ";",
      "dateFormat": "yyyy-MM-dd",
      "partitionBy": ["RateDate"]
    },
    {
      "name": "Operation",
      "path": "data_files\\csv\\Operations.csv",
      "schema": "data_files\\json\\OperationsSchema.json",
      "sep": ";",
      "dateFormat": "yyyy-MM-dd",
      "partitionBy": ["DateOp"]
    },
    {
      "name": "Account",
      "path": "data_files\\csv\\Accounts.csv",
      "schema": "data_files\\json\\AccountsSchema.json",
      "sep": ";",
      "dateFormat": "yyyy-MM-dd",
      "partitionBy": ["DateOpen"]
    },
    {
      "name": "Client",
      "path": "data_files\\csv\\Clients.csv",
      "schema": "data_files\\json\\ClientsSchema.json",
      "sep": ";",
      "dateFormat": "dd.MM.yyyy",
      "partitionBy": ["RegisterDate"]
    }
  ]
}
